@model EvidencePojisteni.Models.Insurance

@{
    ViewData["Title"] = "Detail pojištění";
}

<div>
    <h3>Detaily pojištění</h3>
    <br />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.InsuranceName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.InsuranceName)
        </dd>
    </dl>
</div>
<div class="text-center p-3">
    <a class="btn btn-lg btn-warning text-white mx-1" asp-action="Edit" asp-route-id="@Model.InsuranceId">Editovat pojištění</a>
    <a class="btn btn-lg btn-danger mx-1" asp-action="Delete" asp-route-id="@Model.InsuranceId">Odstranit pojištění</a>
</div>
